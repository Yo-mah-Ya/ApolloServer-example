{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^18.12.1",
    "yarn": "^1.22"
  },
  "scripts": {
    "all": "yarn clean && yarn compile && yarn start",
    "compile": "tsc -b",
    "clean": "rimraf ./build && rimraf ./coverage && rimraf tsconfig.tsbuildinfo",
    "codegen": "graphql-codegen --config src/resolvers/codegen.yml",
    "lint": "yarn eslint && yarn prettier",
    "eslint": "eslint --fix './**/*.{ts,tsx,js,jsx}'",
    "prettier": "prettier --write './**/*.{ts,tsx,js,jsx,json}'",
    "start": "node --max-http-header-size=24820 ./build/src/index.js",
    "test": "jest --coverage --passWithNoTests --maxWorkers=7",
    "ts-node": "ts-node",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile"
  },
  "main": "build/src/index.js",
  "devDependencies": {
    "@apollo/datasource-rest": "6.0.0",
    "@apollo/server": "4.7.1",
    "@aws-sdk/client-dynamodb": "3.329.0",
    "@aws-sdk/lib-dynamodb": "3.329.0",
    "@graphql-codegen/add": "4.0.1",
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-resolvers": "3.2.1",
    "@graphql-tools/schema": "9.0.19",
    "@opensearch-project/opensearch": "1.2.0",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/jest": "29.5.1",
    "@types/lodash.isplainobject": "4.0.7",
    "@types/node": "20.1.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "dataloader": "2.2.2",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "fp-ts": "2.15.0",
    "graphql": "16.6.0",
    "graphql-constraint-directive": "v2",
    "graphql-depth-limit": "1.1.0",
    "graphql-tag": "2.12.6",
    "io-ts": "2.2.20",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  }
}
